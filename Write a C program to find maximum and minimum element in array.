#include<stdio.h>
int main(){

    int x;

    printf("Enter size of array : ");

    scanf("%d",&x);

    int prince[x],i,max,min;

    printf("Enter elements of Array : ");

    for(i=0; i<x; i++){

        scanf("%d",&prince[i]);

    }

    max=prince[0];

    min=prince[0];

    for(i=0;i<x ;i++){
        if(prince[i]>max){
            max=prince[i];
        }
        if(prince[i]<min){
            min=prince[i];
        }

    }

    printf("Maximum element is : %d\n",max);

    printf("Minimum element is : %d",min);

    return 0;

}

